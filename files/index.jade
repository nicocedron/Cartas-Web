extends template


block content
	.header
		ul.nav
			li#descubre.active Descubre
			li#comparte Comparte
			li#coleciona Colecciona
	+pageContent
		+page
			.cards
				each val in card
					.card(class!="cat_#{val.category}")
						.image(style!="background-image: url(#{val.image})")
						.title #{val.title}
							b En #{val.from}
						.tag #{val.tag}
						.address #{val.address}
						.share Invitar
		+page
			h1 Naty LINDA!!! :D
		+page
			div page 3